{
  "name": "elasticsearch-basics",
  "version": "1.0.0",
  "description": "Elasticsearch Basic Search Training materials with Docker and Node.js",
  "main": "scripts/loadDataset.js",
  "scripts": {
    "load-dataset": "node scripts/loadDataset.js",
    "clean-dataset": "node scripts/cleanDataset.js",
    "start:server": "cd src/server && npm start",
    "start:client": "cd src/client && npm run dev",
    "dev:server": "cd src/server && npm run dev",
    "dev:client": "cd src/client && npm run dev",
    "install:all": "npm install && cd src/server && npm install && cd ../client && npm install",
    "build:client": "cd src/client && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "elasticsearch",
    "kibana",
    "docker",
    "search",
    "training",
    "tmdb",
    "react",
    "websocket",
    "movie-search"
  ],
  "author": "Abdul Qadeer Khan",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^8.8.2",
    "inquirer": "^8.2.6",
    "csvtojson": "^2.0.10",
    "node-fetch": "^2.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qadeerk/Elastic-Search-basics.git"
  },
  "bugs": {
    "url": "https://github.com/qadeerk/Elastic-Search-basics/issues"
  },
  "homepage": "https://github.com/qadeerk/Elastic-Search-basics#readme"
} 